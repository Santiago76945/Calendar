{
  "name": "calendar",
  "version": "1.0.0",
  "private": true,
  "description": "App de calendario con Google Calendar (Next.js + Express)",
  "main": "server/app.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:frontend": "cd client && next dev -p 3000",
    "dev:backend": "node server/app.js"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.18.2",
    "googleapis": "^149.0.0",
    "next": "15.3.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwindcss": "^4.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@tailwindcss/postcss": "^4.0.0",
    "concurrently": "^9.1.2",
    "eslint": "^9.0.0",
    "eslint-config-next": "15.3.3",
    "rimraf": "^5.0.0",
    "typescript": "^5.0.0"
  }
}
